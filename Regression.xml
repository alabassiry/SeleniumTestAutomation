<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression" verbose="2">
	<test name="AdvancePlus on Edge">
		<parameter name="browser" value="edge" />
		<parameter name="URL" value="https://www.facebook.com/" />
		<parameter name="state" value="online" />
		<parameter name="tester" value="marwan" />
		<parameter name="executionCycle" value="TST-R1" />
		<classes>
			<class name="helper.TesterPresent" />
			<class name="com.AdvancePlus.Test.TST_T1" />
			<class name="com.AdvancePlus.Test.TST_T2" />
			<class name="com.AdvancePlus.Test.TST_T3" />
		</classes>
	</test>
		<test name="AdvancePlus on Chrome">
		<parameter name="browser" value="chrome" />
		<parameter name="URL" value="https://www.facebook.com/" />
		<parameter name="state" value="online" />
		<parameter name="tester" value="marwan" />
		<parameter name="executionCycle" value="TST-R1" />
		<classes>
			<class name="helper.TesterPresent" />
			<class name="com.AdvancePlus.Test.TST_T1" />
			<class name="com.AdvancePlus.Test.TST_T2" />
			<class name="com.AdvancePlus.Test.TST_T3" />
		</classes>
	</test>
		<test name="AdvancePlus on Firefox">
		<parameter name="browser" value="firefox" />
		<parameter name="URL" value="https://www.facebook.com/" />
		<parameter name="state" value="online" />
		<parameter name="tester" value="marwan" />
		<parameter name="executionCycle" value="TST-R1" />
		<classes>
			<class name="helper.TesterPresent" />
			<class name="com.AdvancePlus.Test.TST_T1" />
			<class name="com.AdvancePlus.Test.TST_T2" />
			<class name="com.AdvancePlus.Test.TST_T3" />
		</classes>
	</test>
</suite>